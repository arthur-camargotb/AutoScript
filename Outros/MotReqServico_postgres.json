  "dsSql" : "SELECT RQM.CDREQUISICAOSERVMOTIVO\r\n\t,RQM.DSREQUISICAOSERVMOTIVO\r\n\t,RQM.FLUSOMOTIVO\r\n\t,CASE \r\n\t\tWHEN RQM.FLUSOMOTIVO = '0'\r\n\t\t\tTHEN 'Todos [0]'\r\n\t\tWHEN RQM.FLUSOMOTIVO = '1'\r\n\t\t\tTHEN 'Na abertura de uma requisiçao de serviço [1]'\r\n\t\tWHEN RQM.FLUSOMOTIVO = '2'\r\n\t\t\tTHEN 'Na resposta de uma requisição de serviço [2]'\r\n\t\tEND DsUSOMOTIVO\r\n\t,RQM.FLOBRIGAOBSERVACAO\r\n\t,CASE \r\n\t\tWHEN RQM.FLSTATUSREQUISICAO = 'P'\r\n\t\t\tTHEN 'Pendente [P]'\r\n\t\tWHEN RQM.FLSTATUSREQUISICAO = 'A'\r\n\t\t\tTHEN 'Em andamento [A]'\r\n\t\tWHEN RQM.FLSTATUSREQUISICAO = 'E'\r\n\t\t\tTHEN 'Encerrada [E]'\r\n\t\tWHEN RQM.FLSTATUSREQUISICAO = 'C'\r\n\t\t\tTHEN 'Cancelada [C]'\r\n\t\tEND FLSTATUSREQUISICAO\r\n\t,RQM.FLATIVO\r\nFROM TBLVWREQUISICAOSERVMOTIVO RQM\r\nWHERE (\r\n\t\tUPPER(RQM.DSREQUISICAOSERVMOTIVO) LIKE '%' || UPPER(:DSREQUISICAOSERVMOTIVO) || '%'\r\n\t\tOR :DSREQUISICAOSERVMOTIVO IS NULL\r\n\t\t)\r\n\tAND (\r\n\t\tRQM.FLUSOMOTIVO = :FLUSOMOTIVO\r\n\t\tOR :FLUSOMOTIVO IS NULL\r\n\t\t)\r\n\tAND (\r\n\t\tRQM.FLSTATUSREQUISICAO = :FLSTATUSREQUISICAO\r\n\t\tOR :FLSTATUSREQUISICAO IS NULL\r\n\t\t)\r\n\tAND (\r\n\t\tRQM.FLATIVO = :FLATIVO\r\n\t\tOR :FLATIVO IS NULL\r\n\t\t)",
